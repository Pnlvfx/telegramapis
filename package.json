{
  "name": "telegramapis",
  "description": "Node telegram Bot API.",
  "version": "2023.10.4",
  "author": {
    "email": "simonegauli@gmail.com",
    "name": "Simone Gauli"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    "types": "./dist/esm/index.d.ts",
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js"
  },
  "type": "module",
  "license": "MIT",
  "keywords": [
    "telegram",
    "telegram bot",
    "telegram bot api",
    "bot",
    "telegram api"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pnlvfx/telegramapis.git"
  },
  "scripts": {
    "tsc": "tsc",
    "generate:package.json": "tsx ./tools/generate-commonjs-package-json.ts dist/cjs/package.json",
    "dev": "tsc -w & node --env-file .env --watch ./dist/esm/dev.js",
    "start": "node .",
    "lint": "eslint .",
    "build": "rimraf dist && eslint . && tsc && npm run build:cjs && npm run generate:package.json",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --moduleResolution node --outDir dist/cjs",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint",
    "test": "jest --coverage"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.5.5",
    "dotenv": "^16.4.5",
    "eslint": "^9.10.0",
    "eslint-plugin-sonarjs": "^2.0.2",
    "eslint-plugin-unicorn": "^55.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.5.0"
  },
  "peerDependencies": {
    "coraline": "github:Pnlvfx/coraline",
    "form-data": "^4.0.0"
  }
}
