  "license": "MIT",
  "type": "module",
  "module": "./dist/telegram.js",
  "types": "./dist/telegram.d.ts",
  "exports": {
    "types": "./dist/telegram.d.ts",
    "import": "./dist/telegram.js"
  },
  "scripts": {
    "tsc": "tsc --noEmit true",
    "unused": "node ./tools/unused.ts",
