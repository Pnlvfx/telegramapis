    "build": "rimraf dist && eslint && tsc -p tsconfig.build.json",
    "prepare": "husky && npm run build",
    "prepack": "yarn build",
    "prepublishOnly": "yarn test && yarn lint"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@eslint/js": "^9.19.0",
