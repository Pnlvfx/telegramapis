{
  "name": "telegramapis",
  "description": "Node telegram Bot API.",
  "version": "2023.10.4",
  "author": {
    "email": "simonegauli@gmail.com",
    "name": "Simone Gauli"
  },
  "main": "./dist/esm/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./*": {
      "import": "./*",
      "require": "./*"
    }
  },
  "type": "module",
  "license": "MIT",
  "keywords": [
    "telegram",
    "telegram bot",
    "telegram bot api",
    "bot",
    "telegram api"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pnlvfx/telegramapis.git"
  },
  "scripts": {
    "generate:package.json": "tsx ./tools/generate-commonjs-package-json.ts dist/cjs/package.json",
    "dev": "tsc -w & node --env-file .env --watch .",
    "start": "node .",
    "lint": "eslint ./src",
    "build": "rimraf dist && npm run lint && npm run build:esm && npm run build:cjs && npm run generate:package.json",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --moduleResolution node --outDir dist/cjs",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --verbose --coverage"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.11",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-sonarjs": "^1.0.3",
    "eslint-plugin-unicorn": "^54.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.2",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.0-alpha.41"
  },
  "peerDependencies": {
    "form-data": "^4.0.0"
  }
}
