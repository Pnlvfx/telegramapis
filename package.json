    "ts-node": "^10.9.2",
    "ts-unused-exports": "^11.0.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0"
  },
  "peerDependencies": {
    "@goatjs/core": "*",
    "@goatjs/node": "*"
  },
